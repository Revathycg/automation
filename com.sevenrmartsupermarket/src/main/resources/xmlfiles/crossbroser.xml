<suite name="testes" thread-count="5" parallel="tests">
	<listeners>
	<listener class-name="com.sevenrmartsupermarket.listeners.ExtentReporter">
		
	</listener>
	</listeners>
	<test name="running in chrome">
		<parameters>
			<parameter name="browser" value="chrome"></parameter>
		</parameters>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.LoginTest"></class>
		</classes>
	</test>
	<test name="running in edge">
		<parameters>
			<parameter name="browser" value="edge"></parameter>
		</parameters>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.LoginTest"></class>
		</classes>
	</test>
	<test name="running in firefox">
		<parameters>
			<parameter name="browser" value="firefox"></parameter>
		</parameters>
		<classes>
			<class name="com.sevenrmartsupermarket.tests.LoginTest"></class>
		</classes>
	</test>
</suite>